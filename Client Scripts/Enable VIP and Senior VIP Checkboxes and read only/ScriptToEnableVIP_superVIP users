function onLoad() {
   //Type the appropriate comment here, and begin the script below
   var myRole= g_user.hassle("admin");
	if(myRole!=true){
		g_form.setReadOnly("VIP",true);
		g_form.setReadOnly("u_senior_vip",true);
	}else{
		g_form.setReadOnly("VIP",false);
		g_form.setReadOnly("u_senior_vip",false);
	}
}
