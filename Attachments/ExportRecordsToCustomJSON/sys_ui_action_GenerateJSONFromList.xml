<name>Generate FCAC JSON Via List</name>
<onclick>confirmAndGenerateFromList()</onclick>
<order>100</order>
<script><![CDATA[var tblName;
var ajaxHelper;
var selSysIds;
var sysIdList;
var indx = 0;
var numSel = 0;

function confirmAndGenerateFromList() {
    tblName = g_list.getTableName();
    selSysIds = g_list.getChecked();
    sysIdList = selSysIds.split(',');
    numSel = sysIdList.length;
    
    if(numSel > 0) {
        indx = 0;
        ajaxHelper = new GlideAjax('GenerateJSONHelper');
		ajaxHelper.addParam('sysparm_name', 'generateJSONForAttachments');
		ajaxHelper.addParam('sysparm_id_list', selSysIds);
		ajaxHelper.getXMLAnswer(displayAnswer, null, null);
        //ajaxHelper.generateJSONForAttachments(selSysIds);
    }
}

function displayAnswer(answer){
	if(answer == 'true'){
		alert('JSON export will be emailed to you shortly.');
	}
}]]></script>
